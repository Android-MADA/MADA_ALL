        android:name="com.example.myapplication.StartFuction.Splash2Activity"
        android:label="splash2"
        tools:layout="@layout/splash2" />
    <fragment
        android:id="@+id/categoryInActiveFragment"
        android:name="com.example.myapplication.HomeFunction.view.CategoryInActiveFragment"
