            android:id="@+id/action_fragCalendar_to_fragCalendar"
            app:destination="@id/fragCalendar" />
    </fragment>
    <fragment
        android:id="@+id/calendarAdd"
        android:name="com.example.myapplication.CalenderFuntion.CalendarAddFragment"
