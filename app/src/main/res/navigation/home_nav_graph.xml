            android:id="@+id/action_myPremiumFragment_to_fragMy"
            app:destination="@id/fragMy" />
    </fragment>
    <fragment
        android:id="@+id/myWithdrawFragment"
        android:name="com.example.myapplication.MyFuction.Fragment.MyWithdrawFragment"
