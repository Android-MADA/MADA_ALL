<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="false" android:state_pressed="false">
        <shape>
            <solid android:color="#F5F5F5"/>
            <corners android:topLeftRadius="100dp" android:topRightRadius="300dp"
                android:bottomLeftRadius="100dp" android:bottomRightRadius="300dp" />
            <stroke android:width="1dp" android:color="#232323" android:dashWidth="7dp" android:dashGap="7dp"/>
        </shape>

    </item>
</selector>